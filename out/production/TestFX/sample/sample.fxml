   <children>
      <Button mnemonicParsing="false" onAction="#handleBtn" prefHeight="25.0" prefWidth="149.0" text="押してみよう" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <CheckBox fx:id="checkBox1" mnemonicParsing="false" text="CheckBox" onAction="#checkBoxChanged" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label fx:id="label1" text="" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   </children>
</GridPane>
